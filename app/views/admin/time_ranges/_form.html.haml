= form_for [:admin, @time_range ||= TimeRange.new] do |f|
  - if @time_range.errors.any?
    #error_explanation
      %h2= "#{pluralize(@time_range.errors.count, "error")} prohibited this admin_time_range from being saved:"
      %ul
        - @time_range.errors.full_messages.each do |msg|
          %li= msg

  = f.text_field :during        

  .actions
    = f.submit 'Save'
